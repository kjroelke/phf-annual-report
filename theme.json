{
	"version": 3,
	"$schema": "https://schemas.wp.org/trunk/theme.json.",
	"settings": {
		"appearanceTools": true,
		"color": {
			"background": true,
			"customDuotone": false,
			"customGradient": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"palette": [
				{
					"slug": "primary",
					"color": "#542988",
					"name": "Primary"
				},
				{
					"slug": "secondary",
					"color": "#54595f",
					"name": "Secondary"
				},
				{
					"slug": "accent",
					"color": "#7f5ea6",
					"name": "Accent"
				},
				{
					"slug": "body",
					"color": "#444444",
					"name": "Body"
				}
			],
			"text": true,
			"heading": true,
			"button": true
		},
		"dimensions": true,
		"layout": {
			"contentSize": "640px",
			"wideSize": "1320px"
		},
		"spacing": true,
		"typography": {
			"customFontSize": true,
			"defaultFontSizes": false,
			"lineHeight": true,
			"dropCap": false,
			"fluid": true,
			"fontStyle": true,
			"fontWeight": true,
			"letterSpacing": true,
			"textDecoration": true,
			"textTransform": true,
			"fontSizes": [
				{
					"slug": "xs",
					"size": "0.833rem",
					"name": "XS"
				},
				{
					"slug": "base",
					"size": "1rem",
					"name": "Base"
				},
				{
					"slug": "small",
					"size": "1.2rem",
					"name": "Small (H6)"
				},
				{
					"slug": "medium",
					"size": "1.44rem",
					"name": "Medium (H5)"
				},
				{
					"slug": "large",
					"size": "1.728rem",
					"name": "Large (H4)"
				},
				{
					"slug": "xl",
					"size": "2.074rem",
					"name": "XL (H3)"
				},
				{
					"slug": "2xl",
					"size": "2.488rem",
					"name": "3XL (H2)"
				},
				{
					"slug": "3xl",
					"size": "2.986rem",
					"name": "3XL (H1)"
				}
			]
		},
		"custom": {
			"lineHeight": "calc(4px + 2ex)",
			"transitionTiming": ".15s",
			"transitionEasing": "ease-in-out",
			"transition": "var:custom|transitionTiming var:custom|transitionEasing",
			"gutters": "1.25rem"
		}
	},
	"styles": {
		"typography": {
			"fontSize": "var:preset|font-size|base",
			"lineHeight": "var:custom|line-height"
		},
		"blocks": {
			"core/paragraph": {
				"typography": {
					"fontSize": "var:preset|font-size|base",
					"lineHeight": "var:custom|line-height"
				}
			},
			"core/heading": {
				"typography": {
					"lineHeight": "var:custom|line-height"
				}
			}
		},
		"elements": {
			"heading": {
				"lineHeight": "var:custom|line-height"
			},
			"h1": {
				"typography": {
					"fontSize": "var:preset|font-size|3-xl",
					"lineHeight": "var:custom|line-height"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var:preset|font-size|2-xl",
					"lineHeight": "var:custom|line-height"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var:preset|font-size|xl",
					"lineHeight": "var:custom|line-height"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var:preset|font-size|large",
					"lineHeight": "var:custom|line-height"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var:preset|font-size|medium",
					"lineHeight": "var:custom|line-height"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var:preset|font-size|small",
					"lineHeight": "var:custom|line-height"
				}
			}
		}
	}
}
